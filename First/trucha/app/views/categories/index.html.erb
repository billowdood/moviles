<h1>Categorias</h1>
<%@categories.each do |c|%>
	<div class="row">
		<div class="col-lg-1">
			Id: <%=c.id%>
		</div>
		<div class="col-lg-2">
			Nombre: <%=c.name%>
		</div>
		<div class="col-lg-2">
			<%=link_to 'Eliminar',category_path(c.id),method: :delete%>
		</div>
		<div class="col-lg-2">
			<%=link_to 'Editar',edit_category_path(c.id)%>
		</div>
	</div>
<%end%>